{
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "functions": {
    "api/**/*": {
      "includeFiles": "./tina/**/*"
    }
  },
  "headers": [
    {
      "source": "/_next/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/tina/:path*",
      "destination": "/api/tina/backend"
    }
  ]
}
