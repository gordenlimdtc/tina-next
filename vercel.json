{
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/tina/backend.ts": {
      "includeFiles": "tina/__generated__/**"
    },
    "api/cloudinary/[...media].ts": {
      "includeFiles": "tina/__generated__/**"
    }
  },
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/tina/:path*",
      "destination": "/api/tina/backend"
    }
  ]
}
